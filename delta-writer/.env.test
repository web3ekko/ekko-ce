# Test configuration for Delta Writer Service
SERVICE_NAME=transactions-writer-test
METRICS_PORT=9091

# NATS Configuration
NATS_URL=nats://localhost:4222
# Production subject pattern: transactions.{vmtype}.{network}.{subnet}
NATS_SUBJECT=transactions.>
NATS_CONSUMER_NAME=transactions-writer-test
NATS_STREAM_NAME=BLOCKCHAIN

# MinIO Configuration
S3_ENDPOINT=http://localhost:9000
S3_ACCESS_KEY=minioadmin
S3_SECRET_KEY=minioadmin
S3_REGION=us-east-1
S3_BUCKET=blockchain-events
S3_USE_SSL=false

# Delta Lake Configuration
DELTA_TABLE_BASE_PATH=test-events
BATCH_SIZE=10
FLUSH_INTERVAL_SECONDS=5
MAX_CONCURRENT_WRITES=2

# Processing Configuration
WORKER_THREADS=2
BUFFER_SIZE=100
RETRY_ATTEMPTS=3
RETRY_DELAY_MS=1000

# Logging
RUST_LOG=debug,ekko_delta_writer=trace
